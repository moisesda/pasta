// Função : troca de valores entre var
// Autor : Moisés
// Data : 13/03/2024
// Seção de Declarações 
var

a, b, troca : inteiro

inicio

a <- 10
b <- 20
escreval("O valor antes da troca a:",a," / b:",b)
troca <- a
a <- b
b <- troca
escreva("O valor depois da troca a:",a,"/ b:",b)

fimalgoritmo
