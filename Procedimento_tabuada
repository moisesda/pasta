Algoritmo "Pocedimento_tabuada"
var
num : inteiro

procedimento Tabuada(valor : inteiro)
Var

i : inteiro

inicio
   escreval("Tabuada de :",valor)

   para i de 1 ate 10 faca
      escreval(valor," x",i," =",valor*i)
   fimpara
   
fimprocedimento

Inicio

escreva("Digite um n√∫mero: ")
leia(num)
Tabuada(num)

fimalgoritmo
